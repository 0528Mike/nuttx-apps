config EXAMPLES_CAN
	bool "CAN example"
	default n
	---help---
		Enable the CAN example

if EXAMPLES_CAN

config EXAMPLES_CAN_NMSGS
	int "Number of Messages"
	default 32
	---help---
		The number of CAN messages to send before returning

endif

